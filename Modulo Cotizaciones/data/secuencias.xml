<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Secuencia para Proyectos -->
        <record id="seq_proyecto_sicone" model="ir.sequence">
            <field name="name">Secuencia Proyecto SICONE</field>
            <field name="code">sicone.proyecto</field>
            <field name="prefix">PROY/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        
        <!-- Secuencia para Cotizaciones -->
        <record id="seq_cotizacion_sicone" model="ir.sequence">
            <field name="name">Secuencia Cotizaci√≥n SICONE</field>
            <field name="code">sicone.cotizacion</field>
            <field name="prefix">COT/%(year)s/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        
    </data>
</odoo>
